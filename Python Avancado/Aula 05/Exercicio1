def formatar_string(string, formato_funcao):
    string_formatada = formato_funcao(string)
    return string_formatada

def formato_arbitrario(string):
    return f"[[{string.upper()}]]"

minha_string = "hello, world!"
string_formatada = formatar_string(minha_string, formato_arbitrario)

print(string_formatada)  # Sa√≠da: "[[HELLO, WORLD!]]"
